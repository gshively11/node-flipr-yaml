    "flags",
    "configuration",
    "config",
    "flip",
    "flipping"
  ],
  "author": {
    "name": "Grant Shively",
    "email": "gshively11@gmail.com"
  },
  "homepage": "https://github.com/gshively11/node-flipr-yaml",
  "bugs": "https://github.com/gshively11/node-flipr-yaml/issues",
  "license": "MIT",
  "devDependencies": {
    "benchmark": "^1.0.0",
    "chai": "^1.9.1",
    "istanbul": "^0.2.16",
    "jshint": "^2.5.2",
    "mocha": "^1.20.1",
    "mocha-istanbul": "^0.2.0",
    "proxyquire": "^1.0.1",
    "reggie": "^0.2.1",
    "sinon": "^1.10.2",
    "sinon-chai": "^2.5.0"
  },
  "dependencies": {
